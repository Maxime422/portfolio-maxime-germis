{
  "name": "template-project",
  "version": "1.0.0",
  "description": "A template project for scalable web development.",
  "scripts": {
    "test": "jest",
    "test:unit": "jest --testPathPattern=tests/unit",
    "test:integration": "jest --testPathPattern=tests/integration",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "format:check": "prettier . --check",
    "format:write": "prettier . --write",
    "cypress:open": "npx cypress open",
    "cypress:run": "cypress run --spec 'cypress/e2e/**/*.cy.js'",
    "deps:check": "npm outdated",
    "deps:update": "npm update",
    "package:update": "ncu -u",
    "security:check": "npm audit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Maxime422/template-project.git"
  },
  "author": "Maxime Germis",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Maxime422/template-project/issues"
  },
  "homepage": "https://github.com/Maxime422/template-project#readme",
  "devDependencies": {
    "@eslint/js": "^9.24.0",
    "cypress": "^14.3.0",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.5.3"
  },
  "dependencies": {
    "three": "^0.176.0"
  }
}
